{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./node_modules/next/dist/bin/next dev --turbopack -p 9002",
    "genkit:dev": "node ./node_modules/genkit-cli/dist/bin/genkit.js start -- node ./node_modules/tsx/dist/cli.mjs src/ai/dev.ts",
    "genkit:watch": "node ./node_modules/genkit-cli/dist/bin/genkit.js start -- node ./node_modules/tsx/dist/cli.mjs --watch src/ai/dev.ts",
    "genkit:watch:local": "node ./scripts/genkit-watch.js",
    "build": "next build",
    "start": "node ./node_modules/next/dist/bin/next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "deploy": "gcloud builds submit --config cloudbuild.yaml --substitutions=_BACKEND_ID=mediconnect-backend,_REGION=us-east4",
    "deploy:vercel": "npx vercel --prod",
    "deploy:vercel:preview": "npx vercel",
    "vercel:login": "npx vercel login",
    "seed:firestore": "dotenv -e ./.env -- tsx src/lib/seed.ts"
  },
